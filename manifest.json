{
  "name": "BabelFrog",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Learn languages by easily translating selected text",
  "homepage_url": "http://github.com/dergachev/babelfrog",
  "icons": {
    "16": "icons/frog16.png",
    "48": "icons/frog48.png",
    "128": "icons/frog128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": false
  },
  "options_page": "src/options_custom/index.html",
  "browser_action": {
    "default_icon": {
      "19": "icons/frog19.png",
      "38": "icons/frog38.png"
    },
    "default_title": "BabelFrog"
  },
  "permissions": [
    "activeTab"
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+F"
      }
    }
  }
}
